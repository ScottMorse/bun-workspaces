{
  "name": "bun-workspaces",
  "version": "1.0.0-alpha.5",
  "main": "src/index.ts",
  "homepage": "https://github.com/ScottMorse/bun-workspaces#readme",
  "bin": {
    "bun-workspaces": "bin/cli.js"
  },
  "scripts": {
    "cli": "bun --silent run bin/cli.js",
    "cli:dev": "_BW_RUNTIME_MODE=development bun --silent run bin/cli.js",
    "build": "bun run scripts/build.ts",
    "build:test": "BUILD_INCLUDE_TESTS=true bun run scripts/build.ts && bun --cwd=./dist.test test",
    "build:rslib-only": "rslib build",
    "type-check": "tsc --noEmit"
  }
}
